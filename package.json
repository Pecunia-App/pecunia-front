{
  "name": "pecunia-front",
  "version": "0.14.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --configuration testing",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless --configuration testing",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "lint": "ng lint",
    "format": "prettier --write .",
    "generate-token-map": "node scripts/generate-token-map.mjs",
    "build-tokens": "node scripts/build-tokens.mjs",
    "build-color-tokens": "npm run build-tokens && npm run generate-token-map",
    "e2e": "ng e2e --configuration testing",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.7",
    "@angular/compiler": "^21.0.7",
    "@angular/core": "^21.0.7",
    "@angular/forms": "^21.0.7",
    "@angular/platform-browser": "^21.0.7",
    "@angular/platform-browser-dynamic": "^21.0.7",
    "@angular/router": "^21.0.7",
    "jwt-decode": "^4.0.0",
    "ng-zorro-antd": "^21.0.0-next.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.7",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@cypress/schematic": "^5.0.0",
    "@ngx-env/builder": "^21.0.1",
    "@types/jasmine": "~5.1.13",
    "angular-eslint": "21.1.0",
    "baseline-browser-mapping": "^2.9.12",
    "cypress": "15.8.2",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "style-dictionary": "^5.1.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.52.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
